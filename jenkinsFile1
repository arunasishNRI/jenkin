pipeline{
    tools{
        maven 'mymaven'
    }
    agent any
    stages{
        stage('clone the repo'){
            git 'https://github.com/Sonal0409/DevOpsCodeDemo.git'
        }
        stage('compile the code'){
            sh 'mvn compile'
        }
        stage('review the code'){
            sh 'mvn pwd:pwd'
        }
        stage('unit testing'){
            sh 'mvn test'
        }
        stage('build code'){
            sh 'mvn clean install package'
        }
    }
}